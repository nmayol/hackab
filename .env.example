# OpenJustice API Configuration
# Copy this file to .env and fill in your actual values

# Your OpenJustice API key
VITE_OPENJUSTICE_API_KEY=your-api-key-here

# OpenJustice API URL (defaults to staging)
VITE_OPENJUSTICE_API_URL=https://api.staging.openjustice.ai

# Dialog Flow ID - required for streaming
VITE_DIALOG_FLOW_ID=your-dialog-flow-id-here

# Conversation ID - the conversation to use for all messages
VITE_CONVERSATION_ID=your-conversation-id-here
